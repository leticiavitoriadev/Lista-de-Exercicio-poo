#include <iostream>
#include <iomanip>

int main(void){

    using namespace std;

    cout<<"Informe a quantidade que deseja colocar: "<<endl;

    double valor; cin>>valor;

    cout<<"Informe o valor do litro de gasolina: "<<endl;

    double preco; cin>>preco;

    
    cout<< setprecision(2)<< fixed;
    cout<<"Quantidade de litros: "
        <<(valor/preco)
        <<endl;

    
    return 0;

}
